{
    "name": "@nis2shield/express-middleware",
    "version": "0.3.4",
    "description": "NIS2 Compliance Middleware for Express.js - Forensic logging, active defense, and security headers",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "bin": {
        "check-nis2": "./bin/check-nis2.js"
    },
    "files": [
        "dist",
        "bin",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "nis2",
        "compliance",
        "security",
        "express",
        "middleware",
        "logging",
        "audit",
        "gdpr",
        "rate-limiting",
        "forensic",
        "cybersecurity",
        "enterprise",
        "security-headers",
        "siem",
        "session-guard"
    ],
    "author": "NIS2 Shield Team <hello@nis2shield.com>",
    "license": "MIT",
    "homepage": "https://nis2shield.com/express/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nis2shield/express-nis2-middleware.git"
    },
    "bugs": {
        "url": "https://github.com/nis2shield/express-nis2-middleware/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "peerDependencies": {
        "express": "^4.18.0 || ^5.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.6",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.2",
        "express": "^4.18.2",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "zod": "^3.22.4"
    }
}